upload:
  gilt:
    apidoc-spec:
      path: service/service.json

code:
  gilt:
    apidoc-spec:
      version: latest
      generators:
        play_2_3_client: generated/app/ApidocSpec.scala
    apidoc-generator:
      version: latest
      generators:
        play_2_3_client: generated/app/ApidocGenerator.scala
    apidoc-internal:
      version: latest
      generators:
        play_2_x_json: generated/app/ApidocInternal.scala
    apidoc-api:
      version: latest
      generators:
        play_2_3_client: generated/app/ApidocApi.scala
        play_2_x_routes: api/conf/routes
